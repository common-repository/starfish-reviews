{
    "name": "starfishwp/starfish-reviews",
    "description": "Standard Dependencies for Starfish Reviews",
    "authors": [
        {
            "name": "Matt Galloway",
            "email": "webmaster@silvercolt.com"
        }
    ],
  "license": [
    "GPL-2.0-only"
  ],
  "support": {
    "email": "support@starfish.reviews",
    "issues": "https://wordpress.org/support/plugin/starfish-reviews/",
    "source": "https://plugins.svn.wordpress.org/starfish-reviews/",
    "rss": "https://plugins.trac.wordpress.org/log/starfish-reviews/?limit=100&mode=stop_on_copy&format=rss"
  },
    "config": {
        "disable-tls": false,
        "secure-http": false,
        "vendor-dir": "vendor/",
        "optimize-autoloader": true
    },
    "require": {
        "twig/twig": "v3.3.10",
        "freemius/wordpress-sdk": "2.5.0-rc.2"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "3.6.2",
      "wp-coding-standards/wpcs": "2.3.0",
      "friendsofphp/php-cs-fixer": "v2.19.3"
    },
    "scripts": {
      "phpcs": "phpcs --standard=PSR7"
    }
}